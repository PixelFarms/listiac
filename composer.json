{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "algolia/algoliasearch-laravel": "^1.7",
        "barryvdh/laravel-debugbar": "^2.3",
        "dawson/amazon-ecs": "^2.0",
        "garygreen/pretty-routes": "^0.0.6",
        "laravel/framework": "5.4.*",
        "laravel/scout": "^3.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.3.0",
        "lubusin/laravel-decomposer": "^0.1.4",
        "spatie/laravel-artisan-dd": "^1.1",
        "spatie/laravel-sluggable": "^1.4",
        "subbe/semantics3": "^1.0",
        "tcg/voyager": "^0.11.3"
    },
    "require-dev": {
        "crestapps/laravel-code-generator": "2.1.*",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
